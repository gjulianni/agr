{
  "compilerOptions": {
    "target": "esnext", 
    "module": "esnext", 
    "moduleResolution": "node", 
    "esModuleInterop": true, 
    "strict": true,
     "jsx": "react-jsx",
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "outDir": "./dist", 
    "rootDir": "./src" 
  },
  "include": ["src"]
}